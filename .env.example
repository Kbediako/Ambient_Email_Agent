OPENAI_API_KEY=your_openai_api_key
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_PROJECT="interrupt-workshop"
GOOGLE_API_KEY=your_google_api_key
# Optional overrides (default picks Gemini if GOOGLE_API_KEY is set)
# Per-role model controls (can leave unset to use defaults):
EMAIL_ASSISTANT_MODEL=gemini-2.5-pro
EMAIL_ASSISTANT_ROUTER_MODEL=gemini-2.5-pro
EMAIL_ASSISTANT_TOOL_MODEL=gemini-2.5-flash
EMAIL_ASSISTANT_MEMORY_MODEL=gemini-2.5-pro
EVAL_MODEL=gemini-2.5-flash

# Google Custom Search (optional: enables google_search tool)
GOOGLE_CSE_API_KEY=your_google_cse_api_key
GOOGLE_CSE_CX=your_custom_search_engine_cx
TIMEZONE=Australia/Sydney

# Optional: Skip Gmail mark_as_read in demos (default off)
# EMAIL_ASSISTANT_SKIP_MARK_AS_READ=1

# Optional: Auto-accept HITL prompts in tests/demos (default off)
# HITL_AUTO_ACCEPT=1
